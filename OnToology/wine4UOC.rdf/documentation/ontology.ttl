           owl:equivalentClass [ rdf:type owl:Class ;
                                 owl:oneOf ( :Dry
                                             :Light
                                             :Medium
                                             :OffDry
                                             :Sweet
                                           )
                               ] ;
           rdfs:subClassOf :WineTaste ;
           rdfs:comment "sweetness of wine" .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#WineTaste
:WineTaste rdf:type owl:Class ;
           rdfs:subClassOf :WineDescriptor ;
           rdfs:comment "wine taste" .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#Winery
:Winery rdf:type owl:Class ;
        owl:equivalentClass [ rdf:type owl:Restriction ;
                              owl:onProperty :hasRegion ;
                              owl:someValuesFrom :Region
                            ] ;
        rdfs:comment "winery of wine production" ;
        rdfs:sameAs "http://dbpedia.org/ontology/Winery"^^xsd:anyURI .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#Zinfandel
:Zinfandel rdf:type owl:Class ;
           rdfs:subClassOf :Wine ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :hasBody ;
                             owl:allValuesFrom [ rdf:type owl:Class ;
                                                 owl:oneOf ( :Full
                                                             :Medium
                                                           )
                                               ]
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :hasFlavor ;
                             owl:allValuesFrom [ rdf:type owl:Class ;
                                                 owl:oneOf ( :Moderate
                                                             :Strong
                                                           )
                                               ]
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :hasColor ;
                             owl:hasValue :Red
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :hasSugar ;
                             owl:hasValue :Dry
                           ] .


