#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/license
<http://purl.org/dc/elements/1.1/license> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/rights
<http://purl.org/dc/elements/1.1/rights> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/license
<http://purl.org/dc/terms/license> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2000/01/rdf-schema#sameAs
rdfs:sameAs rdf:type owl:AnnotationProperty .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#created_by
:created_by rdf:type owl:AnnotationProperty .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#title
:title rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#adjacentRegion
:adjacentRegion rdf:type owl:ObjectProperty ,
                         owl:SymmetricProperty ;
                rdfs:domain :Region ;
                rdfs:range :Region ;
                rdfs:comment "which are the adjacent regions in relation to the region of production" .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#hasAcidity
:hasAcidity rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :hasWineDescriptor ;
            rdf:type owl:FunctionalProperty ;
            rdfs:domain :Wine ;
            rdfs:range :WineAcidity ;
            rdfs:comment "it defines acidity level" .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#hasBody
:hasBody rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf :hasWineDescriptor ;
         rdf:type owl:FunctionalProperty ;
         rdfs:domain :Wine ;
         rdfs:range :WineBody ;
         rdfs:comment "wine body" .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#hasColor
:hasColor rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :hasWineDescriptor ;
          rdf:type owl:FunctionalProperty ;
          rdfs:domain :Wine ;
          rdfs:range :WineColor ;
          rdfs:comment "wine color" .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#hasCountry
:hasCountry rdf:type owl:ObjectProperty ;
            rdfs:domain :Region ;
            rdfs:range :Country ;
            rdfs:comment "country of production" .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#hasFlavor
:hasFlavor rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :hasWineDescriptor ;
           rdf:type owl:FunctionalProperty ;
           rdfs:domain :Wine ;
           rdfs:range :WineFlavor ;
           rdfs:comment "wine taste" .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#hasMaker
:hasMaker rdf:type owl:ObjectProperty ;
          owl:inverseOf :producesWine ;
          rdf:type owl:FunctionalProperty ;
          rdfs:domain :Wine ;
          rdfs:range :Winery ;
          rdfs:comment "winery where the wine is produced" .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#hasQuality
:hasQuality rdf:type owl:ObjectProperty ,
                     owl:FunctionalProperty ;
            rdfs:domain :Wine ;
            rdfs:range :Quality .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#hasRegion
:hasRegion rdf:type owl:ObjectProperty ;
           rdfs:domain :Country ;
           rdfs:range :Region ;
           rdfs:comment "region of the country" .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#hasSugar
:hasSugar rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :hasWineDescriptor ;
          rdf:type owl:FunctionalProperty ;
          rdfs:domain :Wine ;
          rdfs:range :WineSugar ;
          rdfs:comment "wine sweetness level" .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#hasVintageYear
:hasVintageYear rdf:type owl:ObjectProperty ,
                         owl:FunctionalProperty ;
                rdfs:domain :Vintage ;
                rdfs:range :VintageYear ;
                rdfs:comment "special year of wine production" .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#hasWineDescriptor
:hasWineDescriptor rdf:type owl:ObjectProperty ;
                   rdfs:domain :Wine ;
                   rdfs:range :WineDescriptor ;
                   rdfs:comment "wine properties" .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#locatedIn
:locatedIn rdf:type owl:ObjectProperty ;
           rdfs:domain :Wine ;
           rdfs:range :Region ;
           rdfs:comment "where is located" .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#producesWine
:producesWine rdf:type owl:ObjectProperty ;
              rdfs:domain :Winery ;
              rdfs:range :Wine ;
              rdfs:comment "winery where the wine is produced" .


#################################################################
#    Data properties
#################################################################

###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#isSparkling
:isSparkling rdf:type owl:DatatypeProperty ,
                      owl:FunctionalProperty ;
             rdfs:domain :Wine ;
             rdfs:range xsd:boolean ;
             rdfs:comment "specifies when a wine is sparkling" .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#price
:price rdf:type owl:DatatypeProperty ,
                owl:FunctionalProperty ;
       rdfs:domain :Wine ;
       rdfs:range xsd:decimal ;
       rdfs:comment "Specifies the wine price" .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#rating
:rating rdf:type owl:DatatypeProperty ,
                 owl:FunctionalProperty ;
        rdfs:domain :Wine ;
        rdfs:range xsd:decimal ;
        rdfs:comment "average of ratings of the wine" .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#reviews
:reviews rdf:type owl:DatatypeProperty ,
                  owl:FunctionalProperty ;
         rdfs:domain :Wine ;
         rdfs:range xsd:int ;
         rdfs:comment "number of reviews of the wine" .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#yearProduction
:yearProduction rdf:type owl:DatatypeProperty ,
                         owl:FunctionalProperty ;
                rdfs:domain :Wine ;
                rdfs:range xsd:positiveInteger ;
                rdfs:comment "wine year of production" .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#yearValue
:yearValue rdf:type owl:DatatypeProperty ;
           rdfs:domain :Wine ;
           rdfs:range xsd:positiveInteger ;
           rdfs:comment "special year for vintage classification" .


#################################################################
#    Classes
#################################################################

###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#Albariño
:Albariño rdf:type owl:Class ;
          rdfs:subClassOf :Wine ;
          rdfs:comment "Albariño wine" .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#AlsatianWine
:AlsatianWine rdf:type owl:Class ;
              owl:equivalentClass [ owl:intersectionOf ( :Wine
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty :locatedIn ;
                                                           owl:hasValue :AlsaceRegion
                                                         ]
                                                       ) ;
                                    rdf:type owl:Class
                                  ] ;
              rdfs:subClassOf :FrenchWine .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#AmericanWine
:AmericanWine rdf:type owl:Class ;
              owl:equivalentClass [ owl:intersectionOf ( :Wine
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty :locatedIn ;
                                                           owl:hasValue :USRegion
                                                         ]
                                                       ) ;
                                    rdf:type owl:Class
                                  ] .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#Anjou
:Anjou rdf:type owl:Class ;
       owl:equivalentClass [ owl:intersectionOf ( :Loire
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty :locatedIn ;
                                                    owl:hasValue :AnjouRegion
                                                  ]
                                                ) ;
                             rdf:type owl:Class
                           ] ;
       rdfs:subClassOf [ rdf:type owl:Restriction ;
                         owl:onProperty :hasBody ;
                         owl:hasValue :Light
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :hasColor ;
                         owl:hasValue :Rose
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :hasFlavor ;
                         owl:hasValue :Delicate
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :hasSugar ;
                         owl:hasValue :OffDry
                       ] .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#Beaujolais
:Beaujolais rdf:type owl:Class ;
            owl:equivalentClass [ owl:intersectionOf ( :Wine
                                                       [ rdf:type owl:Restriction ;
                                                         owl:onProperty :locatedIn ;
                                                         owl:hasValue :BeaujolaisRegion
                                                       ]
                                                     ) ;
                                  rdf:type owl:Class
                                ] ;
            rdfs:subClassOf [ rdf:type owl:Restriction ;
                              owl:onProperty :hasBody ;
                              owl:hasValue :Light
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :hasColor ;
                              owl:hasValue :Red
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :hasFlavor ;
                              owl:hasValue :Delicate
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :hasSugar ;
                              owl:hasValue :Dry
                            ] .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#Bordeaux
:Bordeaux rdf:type owl:Class ;
          owl:equivalentClass [ owl:intersectionOf ( :Wine
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty :locatedIn ;
                                                       owl:hasValue :BordeauxRegion
                                                     ]
                                                   ) ;
                                rdf:type owl:Class
                              ] .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#Burgundy
:Burgundy rdf:type owl:Class ;
          owl:equivalentClass [ owl:intersectionOf ( :Wine
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty :locatedIn ;
                                                       owl:hasValue :BourgogneRegion
                                                     ]
                                                   ) ;
                                rdf:type owl:Class
                              ] ;
          rdfs:subClassOf [ rdf:type owl:Restriction ;
                            owl:onProperty :hasSugar ;
                            owl:hasValue :Dry
                          ] .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#CabernetFranc
:CabernetFranc rdf:type owl:Class ;
               rdfs:subClassOf :Wine ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :hasBody ;
                                 owl:hasValue :Medium
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :hasColor ;
                                 owl:hasValue :Red
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :hasFlavor ;
                                 owl:hasValue :Moderate
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :hasSugar ;
                                 owl:hasValue :Dry
                               ] .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#CabernetSauvignon
:CabernetSauvignon rdf:type owl:Class ;
                   rdfs:subClassOf :Wine ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :hasBody ;
                                     owl:allValuesFrom [ rdf:type owl:Class ;
                                                         owl:oneOf ( :Full
                                                                     :Medium
                                                                   )
                                                       ]
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :hasFlavor ;
                                     owl:allValuesFrom [ rdf:type owl:Class ;
                                                         owl:oneOf ( :Moderate
                                                                     :Strong
                                                                   )
                                                       ]
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :hasColor ;
                                     owl:hasValue :Red
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :hasSugar ;
                                     owl:hasValue :Dry
                                   ] .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#CaliforniaWine
:CaliforniaWine rdf:type owl:Class ;
                owl:equivalentClass [ owl:intersectionOf ( :Wine
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty :locatedIn ;
                                                             owl:hasValue :CaliforniaRegion
                                                           ]
                                                         ) ;
                                      rdf:type owl:Class
                                    ] .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#Chardonnay
:Chardonnay rdf:type owl:Class ;
            rdfs:subClassOf :Wine ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :hasBody ;
                              owl:allValuesFrom [ rdf:type owl:Class ;
                                                  owl:oneOf ( :Full
                                                              :Medium
                                                            )
                                                ]
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :hasFlavor ;
                              owl:allValuesFrom [ rdf:type owl:Class ;
                                                  owl:oneOf ( :Moderate
                                                              :Strong
                                                            )
                                                ]
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :hasColor ;
                              owl:hasValue :White
                            ] .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#CheninBlanc
:CheninBlanc rdf:type owl:Class ;
             rdfs:subClassOf :Wine ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :hasBody ;
                               owl:allValuesFrom [ rdf:type owl:Class ;
                                                   owl:oneOf ( :Full
                                                               :Medium
                                                             )
                                                 ]
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :hasSugar ;
                               owl:allValuesFrom [ rdf:type owl:Class ;
                                                   owl:oneOf ( :Dry
                                                               :OffDry
                                                             )
                                                 ]
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :hasColor ;
                               owl:hasValue :White
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :hasFlavor ;
                               owl:hasValue :Moderate
                             ] .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#Chianti
:Chianti rdf:type owl:Class ;
         rdfs:subClassOf :ItalianWine ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :hasBody ;
                           owl:allValuesFrom [ rdf:type owl:Class ;
                                               owl:oneOf ( :Light
                                                           :Medium
                                                         )
                                             ]
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :hasColor ;
                           owl:hasValue :Red
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :hasFlavor ;
                           owl:hasValue :Moderate
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :hasSugar ;
                           owl:hasValue :Dry
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :locatedIn ;
                           owl:hasValue :ChiantiRegion
                         ] .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#CotesDOr
:CotesDOr rdf:type owl:Class ;
          owl:equivalentClass [ owl:intersectionOf ( :RedBurgundy
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty :locatedIn ;
                                                       owl:hasValue :CotesDOrRegion
                                                     ]
                                                   ) ;
                                rdf:type owl:Class
                              ] ;
          rdfs:subClassOf [ rdf:type owl:Restriction ;
                            owl:onProperty :hasFlavor ;
                            owl:hasValue :Moderate
                          ] .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#Country
:Country rdf:type owl:Class ;
         rdfs:comment "Country where the wine is produced" ;
         rdfs:sameAs "https://dbpedia.org/ontology/Country" .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#DessertWine
:DessertWine rdf:type owl:Class ;
             rdfs:subClassOf :Wine ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :hasSugar ;
                               owl:allValuesFrom [ rdf:type owl:Class ;
                                                   owl:oneOf ( :OffDry
                                                               :Sweet
                                                             )
                                                 ]
                             ] .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#DryRedWine
:DryRedWine rdf:type owl:Class ;
            owl:equivalentClass [ owl:intersectionOf ( :DryWine
                                                       :RedWine
                                                     ) ;
                                  rdf:type owl:Class
                                ] .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#DryRiesling
:DryRiesling rdf:type owl:Class ;
             owl:equivalentClass [ owl:intersectionOf ( :Riesling
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty :hasSugar ;
                                                          owl:hasValue :Dry
                                                        ]
                                                      ) ;
                                   rdf:type owl:Class
                                 ] ;
             rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty :hasBody ;
                               owl:allValuesFrom [ rdf:type owl:Class ;
                                                   owl:oneOf ( :Light
                                                               :Medium
                                                             )
                                                 ]
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :hasColor ;
                               owl:hasValue :White
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :hasFlavor ;
                               owl:hasValue :Delicate
                             ] .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#DryWhiteWine
:DryWhiteWine rdf:type owl:Class ;
              owl:equivalentClass [ owl:intersectionOf ( :DryWine
                                                         :WhiteWine
                                                       ) ;
                                    rdf:type owl:Class
                                  ] .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#DryWine
:DryWine rdf:type owl:Class ;
         owl:equivalentClass [ owl:intersectionOf ( :Wine
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty :hasSugar ;
                                                      owl:hasValue :Dry
                                                    ]
                                                  ) ;
                               rdf:type owl:Class
                             ] .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#EarlyHarvest
:EarlyHarvest rdf:type owl:Class ;
              rdfs:subClassOf :Wine ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :hasSugar ;
                                owl:allValuesFrom [ rdf:type owl:Class ;
                                                    owl:oneOf ( :Dry
                                                                :OffDry
                                                              )
                                                  ]
                              ] ;
              owl:disjointWith :LateHarvest .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#FrenchWine
:FrenchWine rdf:type owl:Class ;
            owl:equivalentClass [ owl:intersectionOf ( :Wine
                                                       [ rdf:type owl:Restriction ;
                                                         owl:onProperty :locatedIn ;
                                                         owl:hasValue :FrenchRegion
                                                       ]
                                                     ) ;
                                  rdf:type owl:Class
                                ] .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#FullBodiedWine
:FullBodiedWine rdf:type owl:Class ;
                owl:equivalentClass [ owl:intersectionOf ( :Wine
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty :hasBody ;
                                                             owl:hasValue :Full
                                                           ]
                                                         ) ;
                                      rdf:type owl:Class
                                    ] .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#Gamay
:Gamay rdf:type owl:Class ;
       rdfs:subClassOf :Wine .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#GermanWine
:GermanWine rdf:type owl:Class ;
            owl:equivalentClass [ owl:intersectionOf ( :Wine
                                                       [ rdf:type owl:Restriction ;
                                                         owl:onProperty :locatedIn ;
                                                         owl:hasValue :GermanyRegion
                                                       ]
                                                     ) ;
                                  rdf:type owl:Class
                                ] .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#IceWine
:IceWine rdf:type owl:Class ;
         owl:equivalentClass [ owl:intersectionOf ( :DessertWine
                                                    :LateHarvest
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty :hasColor ;
                                                      owl:hasValue :White
                                                    ]
                                                  ) ;
                               rdf:type owl:Class
                             ] ;
         rdfs:subClassOf [ rdf:type owl:Restriction ;
                           owl:onProperty :hasBody ;
                           owl:allValuesFrom [ rdf:type owl:Class ;
                                               owl:oneOf ( :Full
                                                           :Medium
                                                         )
                                             ]
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :hasFlavor ;
                           owl:allValuesFrom [ rdf:type owl:Class ;
                                               owl:oneOf ( :Moderate
                                                           :Strong
                                                         )
                                             ]
                         ] .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#ItalianWine
:ItalianWine rdf:type owl:Class ;
             owl:equivalentClass [ owl:intersectionOf ( :Wine
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty :locatedIn ;
                                                          owl:hasValue :ItalianRegion
                                                        ]
                                                      ) ;
                                   rdf:type owl:Class
                                 ] .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#LateHarvest
:LateHarvest rdf:type owl:Class ;
             rdfs:subClassOf :Wine ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :hasFlavor ;
                               owl:allValuesFrom [ rdf:type owl:Class ;
                                                   owl:oneOf ( :Moderate
                                                               :Strong
                                                             )
                                                 ]
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :hasSugar ;
                               owl:hasValue :Sweet
                             ] .


###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#Loire
